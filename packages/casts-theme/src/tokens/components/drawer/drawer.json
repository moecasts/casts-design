        "right": {
          "value": "{space.05-x}"
        }
      }
    },
    "header": {
      "size": {
        "min": {
          "height": {
            "value": "{size.xxlarge}"
          }
        }
      },
      "font": {
        "size": {
          "value": "{font.size.medium}"
        }
      },
      "lineHeight": {
        "value": "{lineHeight.medium}"
      }
    }
  }
