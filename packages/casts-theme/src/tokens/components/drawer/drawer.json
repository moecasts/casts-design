    "color": {
      "surface": {
        "value": "{color.surface.container.default}"
      },
      "border": {
        "value": "{color.border.component.default}"
      }
    },
    "space": {
      "vertical": {
        "value": "{space.05-x}"
      },
      "horizontal": {
        "value": "{space.1-x}"
      }
    },
    "shadow": {
      "value": "{shadow.level.4}"
    },
    "mask": {
      "color": {
